<div class="row row-cols-1 row-cols-md-3 g-4">
	{% for mostLikedPost in mostLikedPosts %}
		<a href="{{ path('app_post_show', {'slug': mostLikedPost.slug, 'username': mostLikedPost.user.username}) }}" class="text-dark">
			{% include "include/forPostLiked.html.twig" %}
		</a>
	{% else %}
		<p>Aucune publication pour le moment.</p>
	{% endfor %}
</div>
